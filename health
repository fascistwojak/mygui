-- Scripts:

local function NLTH_fake_script() -- ImageLabel.LocalScript 
	local script = Instance.new('LocalScript', ImageLabel)

	script.Parent.Image = "http://www.roblox.com/Thumbs/Avatar.ashx?x=100&y=100&userId=" .. game.Players.LocalPlayer.UserId
	
	
	-- POOP CYRUS SCRIPT GUY
end
coroutine.wrap(NLTH_fake_script)()
local function ZKOIYR_fake_script() -- Name.LocalScript 
	local script = Instance.new('LocalScript', Name)

	script.Parent.Text = game.Players.LocalPlayer.Name
	
	
	-- CYRUS SCRIPTER ... PRO CYRS>>>>
end
coroutine.wrap(ZKOIYR_fake_script)()
local function DCMHJN_fake_script() -- Frame_2.LocalScript 
	local script = Instance.new('LocalScript', Frame_2)

	wait(.1)
	while true do
		hp = game.Players.LocalPlayer.Character.Humanoid.Health/100
		script.Parent:TweenSize(UDim2.new(hp,0,1,0),Enum.EasingDirection.Out,Enum.EasingStyle.Quad,0.15)
		wait(.2)
	end
	
	--POOP
end
coroutine.wrap(DCMHJN_fake_script)()
